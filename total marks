#include <stdio.h>

int main() {
    int n, i, j;
    int subjects = 3;  // number of subjects
    printf("Enter number of students: ");
    scanf("%d", &n);

    int marks[n][subjects], total[n];

    for (i = 0; i < n; i++) {
        total[i] = 0;
        printf("\nEnter marks for student %d:\n", i + 1);
        for (j = 0; j < subjects; j++) {
            printf("Subject %d: ", j + 1);
            scanf("%d", &marks[i][j]);
            total[i] += marks[i][j];
        }
    }

    printf("\n--- Total Marks of Students ---\n");
    for (i = 0; i < n; i++) {
        printf("Student %d: %d\n", i + 1, total[i]);
    }

    return 0;
}
