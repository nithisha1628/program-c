#include <stdio.h>
#include <string.h>
int validate(char user[], char pass[]) {
    return (strcmp(user,"admin")==0 && strcmp(pass,"1234")==0);
}
int main() {
    char u[20], p[20];
    printf("Enter username: "); scanf("%s", u);
    printf("Enter password: "); scanf("%s", p);
    if(validate(u,p)) printf("Login Successful\n");
    else printf("Invalid Login\n");
    return 0;
}
